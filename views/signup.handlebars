<div id="signup">
    <h1>Sign Up</h1>
    <form class="signupUser">
        <div>
            <label>Username:</label>
            <input id="userWant" type="text" name="username" />
        </div>
        <div>
            <label>Password:</label>
            <input id="userPass" type="password" name="password" />
        </div>

        <div>
            <label>Confirm Password:</label>
            <input id="passCheck" type="password" name="password" />
        </div>
        <div id="signupBtn">
            <button type="submit">Sign Up</button>
        </div>
    </form>
</div>

<script src="/js/signup.js"></script>
